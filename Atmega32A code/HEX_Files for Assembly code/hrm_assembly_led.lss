                                 .DSEG
000060                           hr_buffer: .BYTE 4    ; Max 3 digits + null
                                 
                                 ; PB0 = RS
                                 ; PB1 = RW
                                 ; PB2 = EN
                                 ; PA4 = D4
                                 ; PA5 = D5
                                 ; PA6 = D6
                                 ; PA7 = D7
                                 ; PC0 = RED LED
                                 ; PC1 = YELLOW LED
                                 ; PC2 = GREEN LED
                                 
                                 .CSEG
                                 .ORG 0x00
000000 c000                      RJMP RESET
                                 
                                 ; ===================== RESET VECTOR =====================
                                 RESET:
                                     ; Setup Stack
000001 e008                          LDI R16, HIGH(RAMEND)
000002 bf0e                          OUT SPH, R16
000003 e50f                          LDI R16, LOW(RAMEND)
000004 bf0d                          OUT SPL, R16
                                 
                                     ; Setup PORTC for LEDs (PC0-PC2)
000005 e017                          LDI R17, 0b00000111
000006 bb14                          OUT DDRC, R17
000007 2711                          CLR R17
000008 bb15                          OUT PORTC, R17
                                 	
                                 	; SETUP PORTB FOR LCD CONTROL
000009 e017                      	LDI R17,0b00000111
00000a bb17                      	OUT DDRB,R17
                                 
                                 	; SETUP PORTA FOR LCD DATA
00000b ef10                      	LDI R17,0b11110000
00000c bb1a                      	OUT DDRA,R17
                                 
00000d d038                      	RCALL LCD_init
00000e d07f                      	RCALL LCD_message_Waiting_HR
00000f 2799                      	CLR R25    ; R25 = 0 ? no HR input yet
                                 
                                     ; USART Init
000010 e100                          LDI R16, (1 << RXEN)
000011 b90a                          OUT UCSRB, R16
000012 e806                          LDI R16, (1 << URSEL) | (1 << UCSZ1) | (1 << UCSZ0)
000013 bd00                          OUT UCSRC, R16
000014 e303                          LDI R16, 51         ; 9600 baud @ 8 MHz
000015 b909                          OUT UBRRL, R16
000016 2700                          CLR R16
000017 bd00                          OUT UBRRH, R16
                                 
                                 Main_Loop:
                                     ; Point X to hr_buffer
000018 e6a0                          LDI R26, LOW(hr_buffer)
000019 e0b0                          LDI R27, HIGH(hr_buffer)
                                 
00001a d00d                          RCALL USART_receive_string   ; fills buffer
                                 
                                     ; Check if first HR input
00001b 3090                          CPI R25, 0
00001c f419                          BRNE Skip_HR_Message
                                 
00001d d03b                          RCALL LCD_clear
00001e d085                          RCALL LCD_message_Heart_Rate
00001f e091                          LDI R25, 1   ; Set flag: first input handled
                                 
                                 Skip_HR_Message:
000020 d015                          RCALL atoi                   ; result in R16
000021 d09a                          RCALL LED_check              ; checks and lights LED
000022 d0b4                          RCALL LCD_display_HR_on_line2
000023 cff4                          RJMP Main_Loop
                                 
                                 ; ===================== USART RECEIVE CHAR =====================
                                 USART_receive_char:
                                 WaitChar:
000024 9b5f                          SBIS UCSRA, RXC
000025 cffe                          RJMP WaitChar
000026 b10c                          IN R16, UDR
000027 9508                          RET
                                 
                                 ; ===================== RECEIVE STRING INTO BUFFER =====================
                                 ; Expects R26:R27 = X = start of hr_buffer
                                 USART_receive_string:
000028 2722                          CLR R18      ; i = 0
                                 NextChar:
000029 dffa                          RCALL USART_receive_char     ; result in R16
00002a 300d                          CPI R16, 0x0D                ; check for '\r'
00002b f039                          BREQ End_String
00002c 300a                          CPI R16, 0x0A                ; check for '\n'
00002d f029                          BREQ End_String
                                 
00002e 3023                          CPI R18, 3                   ; if i >= 3, ignore extra
00002f f7c8                          BRSH NextChar
                                 
000030 930d                          ST X+, R16                   ; buffer[i++] = R16
000031 9523                          INC R18
000032 cff6                          RJMP NextChar
                                 
                                 End_String:
000033 e000                          LDI R16, 0
000034 930c                          ST X, R16                    ; Null terminate
000035 9508                          RET
                                 
                                 ; ===================== ATOI: Converts hr_buffer ASCII to R16 number =====================
                                 ; Uses: R16 (result), R17, R18, R19, R20
                                 atoi:
000036 2711                          CLR R17        ; result
000037 e02a                          LDI R18, 10     ; multiplier
000038 e6a0                          LDI R26, LOW(hr_buffer)
000039 e0b0                          LDI R27, HIGH(hr_buffer)
                                 
                                 NextDigit:
00003a 914d                          LD R20, X+
00003b 3040                          CPI R20, 0
00003c f039                          BREQ Done_atoi
00003d 5340                          SUBI R20, '0'
00003e 2f31                          MOV R19, R17
00003f 9f32                          MUL R19, R18
000040 2d10                          MOV R17, R0
